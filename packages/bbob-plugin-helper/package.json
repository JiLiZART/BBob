{
  "name": "@bbob/plugin-helper",
  "version": "2.8.1",
  "description": "Set of utils to help write plugins for @bbob/core",
  "keywords": [
    "bbob",
    "plugin",
    "helper"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": "dist/index.js",
  "browserName": "BbobPluginHelper",
